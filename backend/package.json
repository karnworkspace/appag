{
  "name": "taskmanager-backend",
  "version": "1.0.0",
  "description": "Task Management System Backend API",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "test": "jest --watchAll --coverage",
    "test:ci": "jest --coverage",
    "lint": "eslint src/**/*.js",
    "format": "prettier --write \"src/**/*.js\""
  },
  "keywords": [
    "task-management",
    "gmail-api",
    "nodejs",
    "express",
    "mongodb"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "^4.19.2",
    "mongoose": "^8.5.0",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "dotenv": "^16.4.5",
    "express-session": "^1.18.0",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "googleapis": "^140.0.0",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "express-rate-limit": "^7.3.1",
    "express-validator": "^7.1.0",
    "morgan": "^1.10.0",
    "winston": "^3.13.0",
    "node-cron": "^3.0.3",
    "ical.js": "^2.0.0",
    "nodemailer": "^6.9.14",
    "socket.io": "^4.7.5",
    "redis": "^4.6.15",
    "multer": "^1.4.5-lts.1",
    "compression": "^1.7.4"
  },
  "devDependencies": {
    "nodemon": "^3.1.4",
    "jest": "^29.7.0",
    "supertest": "^7.0.0",
    "eslint": "^9.7.0",
    "prettier": "^3.3.3",
    "@types/node": "^20.14.11"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "js,json",
    "ignore": [
      "src/**/*.test.js"
    ],
    "env": {
      "NODE_ENV": "development"
    },
    "execMap": {
      "js": "node --inspect=0.0.0.0:9229"
    }
  }
}
